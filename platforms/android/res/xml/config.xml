<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.HidProductAppNew" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="DatePickerPlugin">
        <param name="android-package" value="com.plugin.datepicker.DatePickerPlugin" />
    </feature>
    <feature name="EmailComposer">
        <param name="android-package" value="de.appplant.cordova.emailcomposer.EmailComposer" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <splash density="land-hdpi" src="www/res/screen/android/splash-land-hdpi.png" />
    <splash density="land-ldpi" src="www/res/screen/android/splash-land-ldpi.png" />
    <splash density="land-mdpi" src="www/res/screen/android/splash-land-mdpi.png" />
    <splash density="land-xhdpi" src="www/res/screen/android/splash-land-xhdpi.png" />
    <splash density="port-hdpi" src="www/res/screen/android/splash-port-hdpi.png" />
    <splash density="port-ldpi" src="www/res/screen/android/splash-port-ldpi.png" />
    <splash density="port-mdpi" src="www/res/screen/android/splash-port-mdpi.png" />
    <splash density="port-xhdpi" src="www/res/screen/android/splash-port-xhdpi.png" />
    <name>HID Product Solutions</name>
    <description>
        A product solutions application for HID Global an ASSA ABLOY company.
    </description>
    <author email="mmcwilliams@hidglobal.com" href="http://www.hidglobal.com">
        HID Global
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon src="www/res/icon.png" />
    <preference name="SplashScreen" value="Default-568h@2x~iphone.png" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="webviewbounce" value="false" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
